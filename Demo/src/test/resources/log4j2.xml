<configuration status="WARN" packages="Samples,com.liaison.commons.log4j2,net.logstash.logging.log4j2.core.layout,com.liaison.commons.audit.log4j2,com.liaison.commons.audit.log4j2" verbose="false">

    <appenders>
        <Console name="Console" target="SYSTEM_OUT" ignoreExceptions="true">
            <PatternLayout pattern="%highlight{%d [%t] %C{1} %-5level: %msg | %X%x%n%throwable}" />
        </Console>
    </appenders>

    <loggers>
        <root level="DEBUG">
            <appender-ref ref="Console" />
        </root>
    </loggers>

</configuration>